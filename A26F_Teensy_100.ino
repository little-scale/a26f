// Setup
void setup() {
  DDRB = B00001111;
  DDRD = B00001111;
  pinMode(wr, OUTPUT);
  digitalWrite(wr, HIGH);
  
  usbMIDI.setHandleControlChange(doCC);
  usbMIDI.setHandleNoteOn(doNote); 
  usbMIDI.setHandleNoteOff(doNoteOff); 
  
  doCC(0, 1, 9);
  doCC(1, 1, 9);
    doCC(1, 1, 9);
  for(int j = 0; j < 8; j++) {
  for(int i = 0; i < 4; i ++) {
    doNote(0, 19 + i + j, 64 + (j * 8));
    doNote(1, 17 + i + j, 64 + (j * 8));
    delay(40);
    doNote(0, 20 + i + j, 0);
    doNote(1, 18 + i + j, 0);
    delay(5);
  }
  

}
  doCC(1, 1, 0);
if(digitalRead(12) == 1) {
  SPB_flag =2;
}

}


// Main Program
