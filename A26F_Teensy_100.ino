// Functions
void doNote(byte channel, byte pitch, byte velocity) {
    if(channel == 1 && (pitch >= 65 && pitch <= 127) ) {
    SPB_sound = (pitch - 65) % 36;
    
    
    if(SPB_sound < 16) {
    SPB_max = 625;
    }
    
    else if(SPB_sound < 19) {
      
      SPB_max = 1250;
    }
    
    else if(SPB_sound < 20) {
      
      SPB_max = 3000;
    }
    
    else if(SPB_sound < 28) {
      
      SPB_max = 500;
    }
    
    else if(SPB_sound < 29) {
      
      SPB_max = 8000;
    } 
    
    
    
    
    if(velocity > 0) {
      SPB_flag = 1;
      SPB_counter = 0;
    }
    
    else {
      SPB_flag = 0;
    }
  }
  
  
  else if(channel == 0 || channel == 1) {
    pitch = 127 - pitch;
    write2600(B01000000 | (channel << 5) | (pitch % 32)); // write to frequency register
    write2600(B10000000 | (channel << 5) | (velocity / 8)); // write to volume register
  }


}

void doNoteOff(byte channel, byte pitch, byte velocity) {
    if(channel == 1 && (pitch >= 65 && pitch <= 127) ) {
            SPB_flag = 0;
    }

 else if(channel == 0 || channel == 1) {
  write2600(B10000000 | (channel << 5));
}
}

  

void doCC(byte channel, byte ccnumber, byte ccvalue) {
  if(channel == 0 || channel == 1) {
    if(ccnumber == 1) {
      write2600(B00000000 | (channel << 5) | (ccvalue / 8));
    }

    else if(ccnumber == 11) {
      write2600(B10000000 | (channel << 5) | (ccvalue / 16));
    }
    else if(ccnumber == 14 && channel == 1) {
      SPB_speed = ccvalue;
  }
  
}
}


